{
	"search.exclude": {
		"export/**/*.hx": true
	},
	"[haxe]": {
		"editor.formatOnSave": true,
		"editor.formatOnPaste": true,
		"editor.codeActionsOnSave": {
            "source.sortImports": "explicit"
        }
	},
    "haxe.enableExtendedIndentation": true,
    "editor.mouseWheelZoom": true,
    "editor.fontSize": 19
}